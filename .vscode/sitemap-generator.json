{
  "sitemap.xml": {
    "Protocol": "https",
    "DomainName": "lel.nekoweb.org",
    "Root": "./",
    "IncludeExt": [".html", ".htm"],
    "Exclude": [
      "*/node_modules/*",
      "*unlighthouse/*",
      "page/sidebar.html",
      "page/template.html"
    ],
    "TagsToInclude": ["priority", "lastmod"],
    "DefaultChangeFrequency": "weekly",
    "TabCharacters": "\t",
    "bIncludeWWW": false,
    "bRemoveFileExtentions": false,
    "bUseTrailingSlash": false,
    "bAutomaticallyUpdateSitemap": false,
    "bMinimized": false
  }
}
