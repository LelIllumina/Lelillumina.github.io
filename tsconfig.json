{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@scripts/*": ["src/scrripts/*"],
      "@assets/*": ["src/assets/*"]
    },
    "types": ["unplugin-icons/types/astro"]
  }
}
